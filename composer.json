{
    "name": "webmaser/wsdl2phpgenerator",
    "description": "Simple utility and class library for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Wsdl2PhpGenerator\\": ["src/", "lib/"]
        }
    },
    "bin": ["wsdl2php"],
    "require": {
        "php": ">=5.3.0",
        "ext-soap": "*",
        "psr/log": "~1.0",
        "symfony/console": "~3.0"
    },
    "require-dev": {
    }
}
